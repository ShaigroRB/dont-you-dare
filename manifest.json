{
    "name": "Don't you dare!",
    "version": "0.1.0",
    "description": "Avoid accidentally closing tabs.",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/listeners.js"
            ]
        }
    ],
    "manifest_version": 2
}